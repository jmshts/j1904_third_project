<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sy.mapper.TbMedRecMapper">
    <insert id="newMedRe" parameterType="com.sy.pojo.TbMedRec">
        insert
        into
        tb_med_rec
        values
        (
        #{mrid},
        #{uid},
        #{did},
        #{username},
        #{usersex},
        #{userage},
        #{idnum},
        #{phone},
        #{date},
        #{department},
        #{istreat},
        #{info},
        #{pillname},
        #{doctor},
        #{ispay}
        )
    </insert>
</mapper>